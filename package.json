{
  "name": "externa",
  "description": "A library to manage communicating between windows, iframes and web workers",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/ifandelse/externa.js.git"
  },
  "homepage": "https://github.com/ifandelse/externa.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Jim Cowart (http://ifandelse.com)",
  "contributors": [
    {
      "name": "Jim Cowart",
      "email": "jim@ifandelse.com",
      "url": "http://ifandelse.com"
    }
  ],
  "keywords": [
    "cross frame",
    "cross window",
    "iframe",
    "worker",
    "web worker",
    "CORS",
    "postMessage",
    "message"
  ],
  "bugs": {
    "email": "jim@ifandelse.com",
    "url": "http://github.com/ifandelse/externa.js/issues"
  },
  "directories": {
    "dist": "dist"
  },
  "main": "dist/externa.js",
  "module": "src/index.js",
  "files": [
    "LICENSE",
    "lib"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "machina": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "dirty-chai": "^2.0.1",
    "eslint-config-leankit": "^6.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^16.6.0",
    "lodash": "^4.17.21",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "regenerator-runtime": "^0.13.7",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "unminified-webpack-plugin": "^3.0.0",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2"
  },
  "license": "MIT",
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "lint": "eslint ./spec ./src --fix",
    "prebuild": "npm run lint",
    "cover": "nyc -r text-summary -r html -- npm run test:only",
    "cover:show": "open \"file://$PWD/coverage/index.html\"",
    "pretest": "npm run lint",
    "test": "npm run cover",
    "test:only": "NODE_ENV=test mocha 'spec/**/*.spec.js'",
    "test:watch": "npm run test:only -- -w"
  },
  "nyc": {
    "exclude": [],
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "require": [
      "@babel/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "mocha": {
    "require": [
      "@babel/register",
      "spec/pre-setup.js",
      "spec/node-setup.js"
    ],
    "reporters": [
      "spec"
    ]
  }
}
